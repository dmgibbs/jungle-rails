<h1>Admin::Categories#index</h1>
<p>Find me in app/views/admin/categories/index.html.erb</p>
<section class="categories-show">

  <header class="page-header">
    <h1>Listing Categories</h1>
  </header>
 <table>
    <tr>
      <th>Categories</th>
    </tr>

    <% render @categories %>
    <h1>The List again<h1>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.name %></td>
      </tr>
    <% end %>
  </table>
  <br/> <br/>
   <div class="well">
    <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-info' %>
  </div>

 

</section>